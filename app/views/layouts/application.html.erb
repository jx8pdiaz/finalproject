<!DOCTYPE html>
<html>
<head>
  <title>Final</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
	<div class="container-fluid nav-top">
		<div class="row">
			<div class="col-xs-2">
				<ul class="list-inline">
				<li><%= link_to "Sign Out", :destroy_user_session , :method => :delete %></li>
				<li><%= link_to "Back", :back%></li>
				<li><%= link_to "Profile", profile_path(current_user) %></li>
				</ul>
			</div>
			<div class="col-xs-9">
			</div>
			<div class="col-xs-1">
				<img src="/minilogo.png" class="minilogo">
			</div>
		</div>
	</div>
<% end %>
<%= yield %>

</body>
</html>
